<div class="module cta-home">

        <div class="row cta-band">
            <div class="col-lg-7">
                <img class="mw-100" src="{{TimberImage(module.image).src|resize(1456,920,'left')}}">
            </div>
            <div class="col-md-9 offset-md-3 cta-content">
                <h2>{{module.heading}}</h2>
                <p class="cta-copy">{{module.copy}}</p>
                {% if module.link %}
                <a class="btn btn-light" href="{{module.link.url}}" target="{{module.link.target}}">{{module.link.title}}</a>
                {% endif %}
            </div>
        </div>
    
</div>